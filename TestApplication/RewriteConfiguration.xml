<?xml version="1.0" encoding="utf-8" ?>
<AssemblyRewriteConfiguration>
  <Assemblies>
    <Assembly>
      <Alias>ExtensibleILRewriter</Alias>
      <Path>ExtensibleILRewriter.dll</Path>
    </Assembly>
  </Assemblies>

  <Types>
    <Type>
      <AssemblyAlias>ExtensibleILRewriter</AssemblyAlias>
      <Alias>NonNullableDefaultHandler</Alias>
      <FullName>ExtensibleILRewriter.MethodProcessors.ArgumentHandling.ArgumentHandlingCodeProvider`1&lt;ArgumentType&gt;</FullName>
    </Type>
  </Types>

  <AssemblyProcessors>
  </AssemblyProcessors>

  <ModuleProcessors>
    <Processor>
      <AssemblyAlias>ExtensibleILRewriter</AssemblyAlias>
      <ProcessorName>ExtensibleILRewriter.ModuleProcessors.ModuleInitializerProcessor</ProcessorName>
    </Processor>
  </ModuleProcessors>

  <TypeProcessors>
  </TypeProcessors>

  <MethodProcessors>
    <Processor>
      <AssemblyAlias>ExtensibleILRewriter</AssemblyAlias>
      <ProcessorName>ExtensibleILRewriter.MethodProcessors.Helpers.MakeStaticVersionProcessor</ProcessorName>
    </Processor>

    <Processor>
      <AssemblyAlias>ExtensibleILRewriter</AssemblyAlias>
      <ProcessorName>ExtensibleILRewriter.MethodProcessors.Contracts.NotNullAttributeProcessor</ProcessorName>
      <Properties>
        <Property>
          <Name>HandlingType</Name>
          <Value>CallStaticHandling</Value>
          <!--<Value>CallInstanceHandling</Value>
          <HandlingInstanceName>NonNullableDefaultHandler</HandlingInstanceName>-->
        </Property>
      </Properties>
    </Processor>
  </MethodProcessors>
</AssemblyRewriteConfiguration>